{
  "name": "lpdb-v3-client",
  "type": "module",
  "version": "0.0.0",
  "description": "A client library for LPDB V3 API",
  "author": "npldevfr",
  "license": "MIT",
  "homepage": "https://github.com/npldevfr/lpdb-v3-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npldevfr/lpdb-v3-client.git"
  },
  "bugs": {
    "url": "https://github.com/npldevfr/lpdb-v3-client/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "generate:types": "openapi-typescript src/types/openapi/spec.json -o src/types/openapi/generated.d.ts",
    "playground": "bun playground/index.ts",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "bumpp": "^10.3.2",
    "openapi-typescript": "^7.10.1",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
