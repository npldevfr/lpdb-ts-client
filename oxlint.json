{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "eqeqeq": "error",
    "no-var": "error",
    "prefer-const": "warn"
  },
  "ignorePatterns": [
    "dist",
    "coverage",
    "node_modules",
    "docs/.vitepress/cache",
    "docs/.vitepress/dist",
    "src/types/openapi/generated.d.ts"
  ]
}
